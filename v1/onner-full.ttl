@prefix : <http://purl.org/spatialai/onner/onner-full#> .
@prefix po: <http://purl.org/spar/po#> .
@prefix bfo: <http://purl.obolibrary.org/obo/bfo.owl#> .
@prefix deo: <http://purl.org/spar/deo#> .
@prefix iao: <http://purl.obolibrary.org/obo/IAO_#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix biro: <http://purl.org/spar/biro#> .
@prefix cito: <http://purl.org/spar/cito#> .
@prefix doco: <http://purl.org/spar/doco#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix fabio: <http://purl.org/spar/fabio#> .
@prefix onner: <http://purl.org/spatialai/onner/onner-full#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@base <http://purl.org/spatialai/onner/onner-full#> .

<http://purl.org/spatialai/onner/onner-full#> rdf:type owl:Ontology ;
                                               owl:versionIRI <http://purl.org/spatialai/onner/v1/onner-full#> ;
                                               dcterms:created "2023-02-05"^^xsd:date ;
                                               dcterms:creator "Torsten Hahmann - http://orcid.org/0000-0002-5331-5052"@en ,
                                                               "Umayer Reza - https://orcid.org/0000-0003-4013-3513"@en ,
                                                               "Xuelian Zhang - https://orcid.org/0000-0002-0708-4070"@en ;
                                               dcterms:description "OnNER is designed to capture and represent named entities — terms identified and labeled using named entity recognition (NER) methods — along with relevant semantic context, such as their location within a document, from scholarly publications. The ontology is aligned with Basic Formal Ontology (BFO) and Information Artifact Ontology (IAO), and it also incorporates relevant high-level concepts from Relation Ontology (RO), Ontology for Biomedical Investigations (OBI), and NCBI organismal classification (NCBITaxon)."@en ;
                                               dcterms:identifier "http://purl.org/spatialai/onner/onner-full"@en ;
                                               dcterms:issued "2024-05-24"^^xsd:date ;
                                               dcterms:license "This work is distributed under a Creative Commons Attribution License (https://creativecommons.org/licenses/by/4.0/)."@en ;
                                               dcterms:modified "2024-05-30"^^xsd:date ;
                                               dcterms:publisher "Spatial Knowledge and Artificial Intelligence (SKAI) Lab, University of Maine"@en ;
                                               dcterms:subject "Information Extraction" ,
                                                               "Named Entity Representation" ,
                                                               "Scholarly Publication" ;
                                               dcterms:title "Ontology for Named Entity Representation in Scholarly Publications (OnNER)"@en ;
                                               owl:versionInfo "1.0"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/contributor
dcterms:contributor rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/created
dcterms:created rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
dcterms:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
dcterms:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/identifier
dcterms:identifier rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/issued
dcterms:issued rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
dcterms:license rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/modified
dcterms:modified rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/publisher
dcterms:publisher rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/rights
dcterms:rights rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/source
dcterms:source rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/subject
dcterms:subject rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
dcterms:title rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#minCardinality
owl:minCardinality rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#definition
<http://www.w3.org/2004/02/skos/core#definition> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#example
<http://www.w3.org/2004/02/skos/core#example> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#Date
xsd:Date rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


###  xsd:anyURI
<xsd:anyURI> rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://purl.obolibrary.org/obo#BFO_0000054
<http://purl.obolibrary.org/obo#BFO_0000054> rdf:type owl:ObjectProperty ;
                                             owl:inverseOf <http://purl.obolibrary.org/obo#BFO_0000055> ;
                                             rdfs:domain <http://purl.obolibrary.org/obo#BFO_0000017> ;
                                             rdfs:range <http://purl.obolibrary.org/obo#BFO_0000015> ;
                                             rdfs:label "realized in"@en .


###  http://purl.obolibrary.org/obo#BFO_0000055
<http://purl.obolibrary.org/obo#BFO_0000055> rdf:type owl:ObjectProperty ;
                                             rdfs:domain <http://purl.obolibrary.org/obo#BFO_0000015> ;
                                             rdfs:range <http://purl.obolibrary.org/obo#BFO_0000017> ;
                                             rdfs:label "realizes"@en .


###  http://purl.obolibrary.org/obo#RO_0000081
<http://purl.obolibrary.org/obo#RO_0000081> rdf:type owl:ObjectProperty ;
                                            owl:inverseOf <http://purl.obolibrary.org/obo#RO_0000087> ;
                                            rdfs:label "role of"@en .


###  http://purl.obolibrary.org/obo#RO_0000087
<http://purl.obolibrary.org/obo#RO_0000087> rdf:type owl:ObjectProperty ;
                                            rdfs:label "has role"@en .


###  http://purl.obolibrary.org/obo#RO_0002234
<http://purl.obolibrary.org/obo#RO_0002234> rdf:type owl:ObjectProperty ;
                                            owl:inverseOf <http://purl.obolibrary.org/obo#RO_0002353> ;
                                            rdfs:label "has output"@en .


###  http://purl.obolibrary.org/obo#RO_0002353
<http://purl.obolibrary.org/obo#RO_0002353> rdf:type owl:ObjectProperty ;
                                            rdfs:label "output of"@en .


###  http://purl.org/spar/biro#isReferencedBy
biro:isReferencedBy rdf:type owl:ObjectProperty ;
                    owl:inverseOf biro:references ;
                    rdfs:label "is referenced by"@en .


###  http://purl.org/spar/biro#references
biro:references rdf:type owl:ObjectProperty ;
                rdfs:label "references"@en .


###  http://purl.org/spar/cito#cites
cito:cites rdf:type owl:ObjectProperty ;
           owl:inverseOf cito:isCitedBy ;
           rdfs:comment "The citing entity cites the cited entity, either directly and explicitly (as in the reference list of a journal article), indirectly (e.g. by citing a more recent paper by the same group on the same topic), or implicitly (e.g. as in artistic quotations or parodies, or in cases of plagiarism)."@en ;
           rdfs:label "cites"@en .


###  http://purl.org/spar/cito#isCitedBy
cito:isCitedBy rdf:type owl:ObjectProperty ;
               rdfs:label "is cited by"@en .


###  http://purl.org/spar/po#contains
po:contains rdf:type owl:ObjectProperty ;
            owl:inverseOf po:isContainedBy ;
            rdfs:label "contains"@en .


###  http://purl.org/spar/po#isContainedBy
po:isContainedBy rdf:type owl:ObjectProperty ;
                 rdfs:label "is contained by"@en .


###  http://purl.org/spatialai/onner/onner-full#assignerOfStatus
onner:assignerOfStatus rdf:type owl:ObjectProperty ;
                       owl:inverseOf onner:statusAssignedBy ;
                       rdfs:domain [ rdf:type owl:Class ;
                                     owl:unionOf ( onner:NER_System
                                                   onner:Person
                                                 )
                                   ] ;
                       rdfs:range onner:LabeledTermStatus ;
                       owl:propertyChainAxiom ( onner:has_NER_LabelerRole
                                                <http://purl.obolibrary.org/obo#BFO_0000054>
                                                <http://purl.obolibrary.org/obo#RO_0002234>
                                              ) ;
                       rdfs:comment "The relationship between an entity (e.g., a person or an NER system) privileged with a NER labeler role and a status of a labeled term (also known as a named entity)."@en ;
                       rdfs:label "assigner of status"@en .


###  http://purl.org/spatialai/onner/onner-full#authorOfPublication
onner:authorOfPublication rdf:type owl:ObjectProperty ;
                          owl:inverseOf onner:publicationAuthoredBy ;
                          rdfs:domain onner:Person ;
                          rdfs:range onner:ScholarlyPublication ;
                          owl:propertyChainAxiom ( onner:hasPublicationAuthorRole
                                                   <http://purl.obolibrary.org/obo#BFO_0000054>
                                                   <http://purl.obolibrary.org/obo#RO_0002234>
                                                 ) ;
                          rdfs:comment "The relationship between a person who holds a publication author role and a document (e.g., a scholarly publication) that a person has written."@en ;
                          rdfs:label "author of publication"@en .


###  http://purl.org/spatialai/onner/onner-full#bibliographicEntryContainedBy
onner:bibliographicEntryContainedBy rdf:type owl:ObjectProperty ;
                                    rdfs:subPropertyOf onner:documentPartContainedBy ;
                                    owl:inverseOf onner:containsBibliographicEntry ;
                                    rdfs:domain onner:BibliographicEntry ;
                                    rdfs:range onner:Bibliography ;
                                    rdfs:comment "The relationship between a detailed source information (e.g., a bibliographic reference) that helps to locate the content of a cited work on the web and a container entity (e.g., a bibliography)."@en ;
                                    rdfs:label "bibliographic entry contained by"@en .


###  http://purl.org/spatialai/onner/onner-full#bibliographicEntryReferencesScholarlyPublication
onner:bibliographicEntryReferencesScholarlyPublication rdf:type owl:ObjectProperty ;
                                                       rdfs:subPropertyOf onner:references ;
                                                       owl:inverseOf onner:scholarlyPublicationReferencedByBibliographicEntry ;
                                                       rdfs:domain onner:BibliographicEntry ;
                                                       rdfs:range onner:ScholarlyPublication ;
                                                       rdfs:comment "The relationship between a detailed source information (e.g., a bibliographic reference) that helps to locate the content of a cited work on the web and a document (e.g., a scholarly publication)."@en ;
                                                       rdfs:label "bibliographic entry references scholarly publication"@en .


###  http://purl.org/spatialai/onner/onner-full#citedBy
onner:citedBy rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf cito:isCitedBy ;
              owl:inverseOf onner:cites ;
              rdf:type owl:AsymmetricProperty ,
                       owl:IrreflexiveProperty ;
              rdfs:domain onner:BibliographicEntry ;
              rdfs:range onner:TextFragment ;
              rdfs:comment "The relationship between a detailed source information (e.g., a bibliographic reference) that helps to locate the content of a cited work on the web and a text fragment citing someone else's work."@en ;
              rdfs:label "cited by"@en .


###  http://purl.org/spatialai/onner/onner-full#cites
onner:cites rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf cito:cites ;
            rdf:type owl:AsymmetricProperty ,
                     owl:IrreflexiveProperty ;
            rdfs:domain onner:TextFragment ;
            rdfs:range onner:BibliographicEntry ;
            rdfs:comment "The relationship between a text fragment citing someone else's work and a detailed source information (e.g., a bibliographic reference) that helps to locate the content of a cited work on the web."@en ;
            rdfs:label "cites"@en .


###  http://purl.org/spatialai/onner/onner-full#containedBy
onner:containedBy rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf po:isContainedBy ;
                  owl:inverseOf onner:contains ;
                  rdfs:label "contained by"@en .


###  http://purl.org/spatialai/onner/onner-full#contains
onner:contains rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf po:contains ;
               rdfs:label "contains"@en .


###  http://purl.org/spatialai/onner/onner-full#containsBibliographicEntry
onner:containsBibliographicEntry rdf:type owl:ObjectProperty ;
                                 rdfs:subPropertyOf onner:containsDocumentPart ;
                                 rdfs:domain onner:Bibliography ;
                                 rdfs:range onner:BibliographicEntry ;
                                 rdfs:comment "The relationship between a container entity (e.g., a bibliography) and a detailed source information (e.g., a bibliographic reference) that helps to locate the content of a cited work on the web."@en ;
                                 rdfs:label "contains bibliographic entry"@en .


###  http://purl.org/spatialai/onner/onner-full#containsDocumentPart
onner:containsDocumentPart rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf onner:contains ;
                           owl:inverseOf onner:documentPartContainedBy ;
                           rdf:type owl:TransitiveProperty ;
                           rdfs:domain [ rdf:type owl:Class ;
                                         owl:unionOf ( onner:DocumentPart
                                                       onner:ScholarlyPublication
                                                     )
                                       ] ;
                           rdfs:range onner:DocumentPart ;
                           rdfs:comment "The relationship between a container entity (e.g., a document or a scholarly publication) and a contained entity (e.g., a document part), regardless of its direct or indirect containment."@en ;
                           rdfs:label "contains document part"@en .


###  http://purl.org/spatialai/onner/onner-full#directlyContainsDocumentPart
onner:directlyContainsDocumentPart rdf:type owl:ObjectProperty ;
                                   rdfs:subPropertyOf onner:containsDocumentPart ;
                                   owl:inverseOf onner:documentPartDirectlyContainedBy ;
                                   rdfs:domain [ rdf:type owl:Class ;
                                                 owl:unionOf ( onner:DocumentPart
                                                               onner:ScholarlyPublication
                                                             )
                                               ] ;
                                   rdfs:range onner:DocumentPart ;
                                   rdfs:comment "The relationship between a container entity (e.g., a document or a scholarly publication) and a directly contained entity (e.g., a document part)."@en ;
                                   rdfs:label "directly contains document part"@en .


###  http://purl.org/spatialai/onner/onner-full#directlyContainsLabeledTerm
onner:directlyContainsLabeledTerm rdf:type owl:ObjectProperty ;
                                  rdfs:subPropertyOf onner:directlyContainsDocumentPart ;
                                  owl:inverseOf onner:labeledTermDirectlyContainedBy ;
                                  rdfs:domain onner:TextFragment ;
                                  rdfs:range onner:LabeledTerm ;
                                  rdfs:comment "The relationship between a container entity (e.g., a text fragment) and a directly contained labeled term (also known as a named entity)."@en ;
                                  rdfs:label "directly contains labeled term"@en .


###  http://purl.org/spatialai/onner/onner-full#documentPartContainedBy
onner:documentPartContainedBy rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf onner:containedBy ;
                              rdf:type owl:TransitiveProperty ;
                              rdfs:domain onner:DocumentPart ;
                              rdfs:range [ rdf:type owl:Class ;
                                           owl:unionOf ( onner:DocumentPart
                                                         onner:ScholarlyPublication
                                                       )
                                         ] ;
                              rdfs:comment "The relationship between a contained entity (e.g., a document part), regardless of its direct or indirect containment and a container entity (e.g., a document or a scholarly publication)."@en ;
                              rdfs:label "document part contained by"@en .


###  http://purl.org/spatialai/onner/onner-full#documentPartDirectlyContainedBy
onner:documentPartDirectlyContainedBy rdf:type owl:ObjectProperty ;
                                      rdfs:subPropertyOf onner:documentPartContainedBy ;
                                      rdfs:domain onner:DocumentPart ;
                                      rdfs:range [ rdf:type owl:Class ;
                                                   owl:unionOf ( onner:DocumentPart
                                                                 onner:ScholarlyPublication
                                                               )
                                                 ] ;
                                      rdfs:comment "The relationship between a directly contained entity (e.g., a document part) and a container entity (e.g., a document or a scholarly publication)."@en ;
                                      rdfs:label "document part directly contained by"@en .


###  http://purl.org/spatialai/onner/onner-full#floatingDocumentPartReferencedByTextFragment
onner:floatingDocumentPartReferencedByTextFragment rdf:type owl:ObjectProperty ;
                                                   rdfs:subPropertyOf onner:referencedBy ;
                                                   owl:inverseOf onner:textFragmentReferencesFloatingDocumentPart ;
                                                   rdfs:domain onner:FloatingDocumentPart ;
                                                   rdfs:range onner:TextFragment ;
                                                   rdfs:comment "The relationship between a floating document element (e.g., a figure, a table, or a listing) and a text fragment (e.g., a paragraph, a caption, or a formula)."@en ;
                                                   rdfs:label "floating document part referenced by text fragment"@en .


###  http://purl.org/spatialai/onner/onner-full#fromLabelingSchema
onner:fromLabelingSchema rdf:type owl:ObjectProperty ,
                                  owl:AsymmetricProperty ,
                                  owl:IrreflexiveProperty ;
                         rdfs:domain onner:Label ;
                         rdfs:range onner:LabelingSchema ;
                         rdfs:comment "The relationship between a classification category (e.g., a label) of a labeled term (also known as a named entity) and a domain-specific schema where a classification category belongs to (e.g., a labeling schema)."@en ;
                         rdfs:label "from labeling schema"@en .


###  http://purl.org/spatialai/onner/onner-full#hasAffiliation
onner:hasAffiliation rdf:type owl:ObjectProperty ,
                              owl:AsymmetricProperty ,
                              owl:IrreflexiveProperty ;
                     rdfs:domain onner:Person ;
                     rdfs:range onner:Organization ;
                     rdfs:comment "The relationship between a person and an organization where a person is affiliated with."@en ;
                     rdfs:label "has affiliation"@en .


###  http://purl.org/spatialai/onner/onner-full#hasCaption
onner:hasCaption rdf:type owl:ObjectProperty ,
                          owl:AsymmetricProperty ,
                          owl:IrreflexiveProperty ;
                 rdfs:domain onner:FloatingDocumentPart ;
                 rdfs:range onner:Caption ;
                 rdfs:comment "The relationship between a floating document element (e.g., a figure, a table, or a listing) and a accompanying text fragment (e.g., a caption) which describes a floating document element."@en ;
                 rdfs:label "has caption"@en .


###  http://purl.org/spatialai/onner/onner-full#hasLabeledTermLabel
onner:hasLabeledTermLabel rdf:type owl:ObjectProperty ,
                                   owl:AsymmetricProperty ,
                                   owl:IrreflexiveProperty ;
                          rdfs:domain onner:LabeledTermStatus ;
                          rdfs:range onner:Label ;
                          rdfs:comment "The relationship between a status of a labeled term (also known as a named entity) and a classification category (e.g., a label) of a labeled term."@en ;
                          rdfs:label "has labeled term label"@en .


###  http://purl.org/spatialai/onner/onner-full#hasLabeledTermStatus
onner:hasLabeledTermStatus rdf:type owl:ObjectProperty ;
                           owl:inverseOf onner:statusOfLabeledTerm ;
                           rdf:type owl:AsymmetricProperty ,
                                    owl:IrreflexiveProperty ;
                           rdfs:domain onner:LabeledTerm ;
                           rdfs:range onner:LabeledTermStatus ;
                           rdfs:comment "The relationship between a labeled term (also known as a named entity) and a status of a labeled term."@en ;
                           rdfs:label "has labeled term status"@en .


###  http://purl.org/spatialai/onner/onner-full#hasPublicationAuthorRole
onner:hasPublicationAuthorRole rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf <http://purl.obolibrary.org/obo#RO_0000087> ;
                               owl:inverseOf onner:roleOfPublicationAuthor ;
                               rdfs:domain onner:Person ;
                               rdfs:range onner:PublicationAuthorRole ;
                               rdfs:comment "The relationship between a person and a role (e.g., a publication author role) that identifies a person as an author of a publication."@en ;
                               rdfs:label "has publication author role"@en .


###  http://purl.org/spatialai/onner/onner-full#has_NER_LabelerRole
onner:has_NER_LabelerRole rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf <http://purl.obolibrary.org/obo#RO_0000087> ;
                          owl:inverseOf onner:roleOf_NER_Labeler ;
                          rdfs:domain [ rdf:type owl:Class ;
                                        owl:unionOf ( onner:NER_System
                                                      onner:Person
                                                    )
                                      ] ;
                          rdfs:range onner:NER_LabelerRole ;
                          rdfs:comment "The relationship between an NER labeler (e.g., a person or an NER system) and a role (e.g., NER labeler role) that grants the privilege of NER labeling to a labeler."@en ;
                          rdfs:label "has NER labeler role"@en .


###  http://purl.org/spatialai/onner/onner-full#labeledTermDirectlyContainedBy
onner:labeledTermDirectlyContainedBy rdf:type owl:ObjectProperty ;
                                     rdfs:subPropertyOf onner:documentPartDirectlyContainedBy ;
                                     rdfs:domain onner:LabeledTerm ;
                                     rdfs:range onner:TextFragment ;
                                     rdfs:comment "The relationship between a directly contained labeled term (also known as a named entity) and a container entity (e.g., a text fragment)."@en ;
                                     rdfs:label "labeled term directly contained by"@en .


###  http://purl.org/spatialai/onner/onner-full#nextDocumentPart
onner:nextDocumentPart rdf:type owl:ObjectProperty ,
                                owl:AsymmetricProperty ,
                                owl:IrreflexiveProperty ;
                       rdfs:domain onner:OrderedDocumentPart ;
                       rdfs:range onner:OrderedDocumentPart ;
                       rdfs:comment "The relationship of one ordered document element (e.g., a section or a paragraph) with another ordered document element."@en ;
                       rdfs:label "next document part"@en .


###  http://purl.org/spatialai/onner/onner-full#publicationAuthoredBy
onner:publicationAuthoredBy rdf:type owl:ObjectProperty ;
                            rdfs:domain onner:ScholarlyPublication ;
                            rdfs:range onner:Person ;
                            owl:propertyChainAxiom ( <http://purl.obolibrary.org/obo#RO_0002353>
                                                     <http://purl.obolibrary.org/obo#BFO_0000055>
                                                     onner:roleOfPublicationAuthor
                                                   ) ;
                            rdfs:comment "The relationship between a document (e.g., a scholarly publication) that a person has written and a person who holds a publication author role."@en ;
                            rdfs:label "publication authored by"@en .


###  http://purl.org/spatialai/onner/onner-full#referencedBy
onner:referencedBy rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf biro:isReferencedBy ;
                   owl:inverseOf onner:references ;
                   rdf:type owl:AsymmetricProperty ,
                            owl:IrreflexiveProperty ;
                   rdfs:label "referenced by"@en .


###  http://purl.org/spatialai/onner/onner-full#references
onner:references rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf biro:references ;
                 rdf:type owl:AsymmetricProperty ,
                          owl:IrreflexiveProperty ;
                 rdfs:label "references"@en .


###  http://purl.org/spatialai/onner/onner-full#roleOfPublicationAuthor
onner:roleOfPublicationAuthor rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf <http://purl.obolibrary.org/obo#RO_0000081> ;
                              rdf:type owl:AsymmetricProperty ,
                                       owl:IrreflexiveProperty ;
                              rdfs:domain onner:PublicationAuthorRole ;
                              rdfs:range onner:Person ;
                              rdfs:comment "The relationship between a role (e.g., a publication author role) that identifies a person as an author of a publication and a person."@en ;
                              rdfs:label "role of publication author"@en .


###  http://purl.org/spatialai/onner/onner-full#roleOf_NER_Labeler
onner:roleOf_NER_Labeler rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf <http://purl.obolibrary.org/obo#RO_0000081> ;
                         rdf:type owl:AsymmetricProperty ,
                                  owl:IrreflexiveProperty ;
                         rdfs:domain onner:NER_LabelerRole ;
                         rdfs:range [ rdf:type owl:Class ;
                                      owl:unionOf ( onner:NER_System
                                                    onner:Person
                                                  )
                                    ] ;
                         rdfs:comment "The relationship between a role (e.g., NER labeler role) that grants the privilege of NER labeling to a labeler and an NER labeler (e.g., a person or an NER system)."@en ;
                         rdfs:label "role of NER labeler"@en .


###  http://purl.org/spatialai/onner/onner-full#scholarlyPublicationReferencedByBibliographicEntry
onner:scholarlyPublicationReferencedByBibliographicEntry rdf:type owl:ObjectProperty ;
                                                         rdfs:subPropertyOf onner:referencedBy ;
                                                         rdfs:domain onner:ScholarlyPublication ;
                                                         rdfs:range onner:BibliographicEntry ;
                                                         rdfs:comment "The relationship between a document (e.g., a scholarly publication) and a detailed source information (e.g., a bibliographic reference) that helps to locate the content of a cited work on the web."@en ;
                                                         rdfs:label "scholarly publication referenced by bibliographic entry"@en .


###  http://purl.org/spatialai/onner/onner-full#statusAssignedBy
